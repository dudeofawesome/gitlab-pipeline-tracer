{
  "name": "gitlab-pipeline-tracer",
  "version": "0.0.0",
  "type": "module",
  "packageManager": "pnpm@9.10.0",
  "license": "MIT",
  "description": "A basic Effect CLI application",
  "repository": {
    "type": "git",
    "url": "https://github.com/dudeofawesome/gitlab-pipeline-tracer"
  },
  "engines": {
    "node": "^24"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "bin": "src/bin.ts",
  "scripts": {
    "build": "tsup && pnpm copy-package-json",
    "build:ts": "tsup",
    "clean": "rimraf dist/*",
    "check": "tsc -b tsconfig.json",
    "lint": "eslint \"**/{src,test,examples,scripts,dtslint}/**/*.{ts,mjs}\"",
    "lint-fix": "pnpm lint --fix",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "copy-package-json": "tsx scripts/copy-package-json.ts"
  },
  "devDependencies": {
    "@effect/cli": "latest",
    "@effect/eslint-plugin": "^0.2.0",
    "@effect/language-service": "^0.27.2",
    "@effect/platform": "latest",
    "@effect/platform-node": "latest",
    "@effect/vitest": "latest",
    "@eslint/compat": "1.1.1",
    "@eslint/eslintrc": "3.1.0",
    "@eslint/js": "9.10.0",
    "@types/common-tags": "^1.8.4",
    "@types/node": "^22.5.2",
    "@typescript-eslint/eslint-plugin": "^8.4.0",
    "@typescript-eslint/parser": "^8.4.0",
    "effect": "latest",
    "eslint": "^9.10.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-codegen": "0.28.0",
    "eslint-plugin-deprecation": "^3.0.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sort-destructure-keys": "^2.0.0",
    "tsup": "^8.2.4",
    "tsx": "^4.19.1",
    "type-fest": "^4.41.0",
    "typescript": "^5.6.2",
    "vitest": "^2.0.5"
  },
  "pnpm": {
    "patchedDependencies": {}
  },
  "dependencies": {
    "@1password/op-js": "^0.1.13",
    "@gitbeaker/rest": "^42.5.0",
    "@mermaid-js/mermaid-cli": "^11.6.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.203.0",
    "@opentelemetry/resources": "^2.0.1",
    "@opentelemetry/sdk-trace-node": "^2.0.1",
    "@opentelemetry/semantic-conventions": "^1.36.0",
    "common-tags": "^1.8.2",
    "dotenv": "^17.2.0"
  }
}
